<%- include('partials/header.ejs') %> 

<div class="cards row">
    
<%foundguests.forEach(function(guest){ %>
    <div class="card col-lg-3" style="width: 20rem;">
        <div class="card-body">
            <p><strong class="text-primary">Email: </strong><%=guest.email%></p>
            <p><strong class="text-primary">Name: </strong><%=guest.name%></p>
            <p><strong class="text-primary">Contact: </strong><%=guest.contact%></p>
            <p><strong class="text-primary">Age: </strong><%=guest.age%></p>
            <p><strong class="text-primary">Address: </strong><%=guest.address%></p> 
        </div>
    </div>
  <% }) %>
</div>